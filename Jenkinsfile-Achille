


pipeline {
    agent any
    environment {
         USERNAME= 'devopseasylearning2021'
        
     }
    options { buildDiscarder(logRotator(artifactDaysToKeepStr: '',
     artifactNumToKeepStr: '', daysToKeepStr: '3', numToKeepStr: '5'))
      disableConcurrentBuilds() }
    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
    
    stage('Hello1') {
            steps {
                sh '''
                pwd
                uname -r
                touch christ
                mkdir bonaberi
                '''
            }
        }
    
    stage('Hello2') {
            steps {
                echo 'Hello World'
            }
        }
    
    stage('Hello3') {
            steps {
                echo 'Hello World'
            }
        }
    
    
    }



}
