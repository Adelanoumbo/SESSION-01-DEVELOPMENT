pipeline {
    agent any
    options {buildDiscarder(logRotator(artifactDaysToKeepStr: '', 
    artifactNumToKeepStr: '', 
    daysToKeepStr: '3', 
    numToKeepStr: '5'))
    disableConcurrentBuilds()}

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hello1') {
            steps {
                sh '''
                ls
                pwd
                uptime 
                echo "The system kernel version is $(uname -r)"
                '''
            }
        }
        stage('Hello2') {
            steps {
                sh '''
                echo 'Hello World2'
                rm -rf groovy-test
                mkdir groovy-test
                cd groovy-test
                touch test1 test2
                '''
            }
        }
    }
}
