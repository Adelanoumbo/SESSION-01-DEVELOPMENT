pipeline {
    agent any
    
    options { buildDiscarder(logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '', daysToKeepStr: '3', numToKeepStr: '5')) 
    disableConcurrentBuilds() }
    
    stages {
        stage('Hello') {
            steps {
                sh '''
                ls
                pwd
                touch cyprien
                mkdir youmbi

                '''
            }
        }

        stage('Hello1') {
            steps {
                sh '''
                uname -r

                '''
            }

        }
    

        stage('Hello2') {
            steps {
                sh '''
                ls -l
                ls -al

                '''
            }

        }
    }

}
