pipeline {
    agent any
    options { buildDiscarder(logRotator(artifactDaysToKeepStr: '',
     artifactNumToKeepStr: '', daysToKeepStr: '3', numToKeepStr: '5'))
      disableConcurrentBuilds() 
      }
    stages {
        stage('Hello') {
            steps {
                sh '''
                touch amy1
                ls 
                pwd
                echo "hello"
                '''
            }
        }
   
        stage('Hello2') {
            steps {
                 sh '''
                touch amy2
                ls -a
                pwd
                echo "hello"
                '''
            }
        }
           
        stage('Hello3') {
            steps {
                sh '''
                touch amy3
                ls -l
                pwd
                echo "hello"
                '''
           
   
            }
        }
    }
}
